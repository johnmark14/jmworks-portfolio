<div class="yb-section-point-wrapper">
    <div class="uk-container">
        <div class="yb-section-point">
        <sup>{{ section.settings.section-number }}</sup>
        <span>{{ section.settings.section-title }}</span>
        </div>
    </div>
</div>

{% schema %}
{
    "name": "Resume Title Number",
    "settings": [
        {
            "type": "text",
            "id": "section-number",
            "label": "Section Number",
            "default": "03"
        },
        {
            "type": "text",
            "id": "section-title",
            "label": "Section Title",
            "default": "resume"
        }
    ]
}
{% endschema %}