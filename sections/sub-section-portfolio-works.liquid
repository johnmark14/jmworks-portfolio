<div>
    <h2 class="yb-section-title uk-heading-line " id="{{-section.settings.section-id-}}"><span>{{section.settings.section-title}}</span></h2>
    <div data-uk-filter="target: .js-filter" class="yb-margin-top-1 yb-margin-bottom-1">
      {% if section.blocks != blank%}
        <ul class="yb-nav-filter uk-subnav uk-subnav-pill ">
            <li><a data-uk-filter-control>All</a></li>
            <li><a data-uk-filter-control=".data-web">Web</a></li>
            <li><a data-uk-filter-control=".data-shopify">Shopify</a></li>
            <li><a data-uk-filter-control=".data-mobile">Mobile</a></li>
        </ul>
      {%endif%}
      <ul class="js-filter uk-child-width-1-2  uk-grid-small  " data-uk-grid data-uk-lightbox="animation: fade;  toggle: .yb-lightbox">
        {% for block in section.blocks %}
            <li class="{% if block.settings.is-web %}data-web{%endif%}
            {% if block.settings.is-shopify%}data-shopify{%endif%}
            {% if block.settings.is-mobile%} data-mobile{%endif%}">
                <div class="yb-work-item uk-transition-toggle">
                    <div class="yb-work-item-img-wrap">
                    <div class="yb-work-item-img">
                        <img src="src/img/empty.png" width="383" 
                        height="574" data-src="{{ block.settings.work-image-preview | img_url: 'master'}}" 
                        class="uk-transition-scale-up uk-transition-opaque" 
                        alt="Work Image {{forloop.index}}" data-uk-img />
                    </div>
                    <div class="yb-work-item-overlay 
                    uk-transition-fade uk-position-cover 
                    uk-position-small 
                    uk-overlay  
                    uk-flex uk-flex-center uk-flex-middle">
                        <div>
                        <a href="{{ block.settings.work-image-full | img_url: 'master'}}" 
                        class="yb-lightbox uk-transition-slide-left" 
                        data-caption="{{ block.settings.project-desc }}">
                            <span data-uk-icon="icon:plus; ratio:1.2"></span>
                        </a>
                        <a href="{{ block.settings.project-url }}" class="uk-transition-slide-right show-portfolio">
                            <span data-uk-icon="icon:link; ratio:1.2"></span>
                        </a>
                        </div>
                    </div>
                    </div>
                    <a href="{{ block.settings.project-url }}" class="show-portfolio">
                    <div class="yb-work-item-title">
                        <h5>{{ block.settings.project-title }}</h5>
                        <span>{{ block.settings.project-type }}</span>
                    </div>
                    </a>
                </div>
            </li>
        {% endfor %}
