{% schema %}
{
    "name": "My Works",
    "settings": [
        {
            "type": "text",
            "id": "section-id",
            "label": "Section ID",
            "default": "myworks"
        },
        {
            "type": "text",
            "id": "section-title",
            "label": "Section Title",
            "default": "My Works"
        }
    ],
    "blocks": [
        {
            "name": "Work",
            "type": "work",
            "settings": [
                {
                    "type": "checkbox",
                    "id": "is-web",
                    "label": "Web Project",
                    "default": true
                },
                {
                    "type": "checkbox",
                    "id": "is-shopify",
                    "label": "Shopify Project",
                    "default": false
                },
                {
                    "type": "checkbox",
                    "id": "is-mobile",
                    "label": "Mobile Project",
                    "default": false
                },
                {
                    "type": "text",
                    "id": "project-title",
                    "label": "Project Title"
                },
                {
                    "type": "text",
                    "id": "project-type",
                    "label": "Project Type"
                },
                {
                    "type": "textarea",
                    "id": "project-desc",
                    "label": "Project Description"
                },
                {
                    "type": "url",
                    "id": "project-url",
                    "label": "Project URL"
                },
                {
                    "type": "header",
                    "content": "Add Image to your work"
                },
                {
                    "type": "image_picker",
                    "id": "work-image-preview",
                    "label": "Preview Image"
                },
                {
                    "type": "image_picker",
                    "id": "work-image-full",
                    "label": "Full Image"
                }
            ]
        }
    ]
}
{% endschema %}