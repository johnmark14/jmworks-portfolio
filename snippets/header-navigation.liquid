<div class="yb-menu-toggle-wrapper" id="menucollapse">
    {% if section.settings.nav-menu %}
        <div class="uk-navbar-center">
            <ul class="uk-navbar-nav" data-uk-scrollspy-nav="closest: li; scroll: false; offset: 79 ">
                {% for link in section.settings.nav-menu.links %}
                    <li>
                        <a class="yb-inner-link" href="{{ link.url }}"> {{ link.title }} </a>
                    </li>
                {% endfor %}
            </ul>
        </div>
    {% endif %}
    <div class="uk-navbar-right">
      <ul class="uk-navbar-nav yb-social-icons">
        <li>
          <a href="{{ section.settings.facebook}}">
            <span class="uk-icon " data-uk-icon="icon: facebook"></span>
          </a>
        </li>
        <li>
          <a href="{{ section.settings.linkedin}}">
            <span class="uk-icon" data-uk-icon="icon: linkedin"></span>
          </a>
        </li>
        <li>
          <a href="{{ section.settings.github}}">
            <span class="uk-icon" data-uk-icon="icon: github"></span>
          </a>
        </li>
      </ul>
    </div>
</div>